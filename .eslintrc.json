{
    "env": {
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "ignorePackages"
            }
        ],
        "no-param-reassign": ["error", { "props": false }]
    }
}
